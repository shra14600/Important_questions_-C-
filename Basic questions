#### Check if a number is positive or negative
import java.io.*;
import java.util.Scanner;
class Code {
	public static void main (String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		if(n>0){
		    System.out.println("Positive number");
		}
		else if(n<0){
		    System.out.println("Negative number");
		}
		else{
		    System.out.println("Zero");
		}
	}
}


#### Check whether a number is even or odd
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a%2==0){
            System.out.println("Even number");
        }
        else{
            System.out.println("Odd number");
        }
    }
}


#### Find the sum of first n natural numbers
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        for(int i=1;i<=n;i++){
            sum+=i;
        }
        System.out.println(sum);
    }
}



#### Greatest of three numbers
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int c=s.nextInt();
        if(a>=b && a>=c){
            System.out.println(a+" is greatest of three");
        }
        else if(b>=a && b>=c){
            System.out.println(b+" is greatest of three");
        }
        else{
            System.out.println(c+" is greatest of three");
        }
    }
}


#### Leap year or not
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        if(n%400==0){
            System.out.println("Leap year");
        }
        else if(n%4==0 && n%100!=0){
            System.out.println("Leap year");
        }
        else{
            System.out.println("Non leap year");
        }
    }
}

 
#### Check whether a number is prime number or not
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        boolean flag=true;
        for(int i=2;i<n;i++){
            if(n%i==0){
                flag=false;
                break;
            }
        }
        if(flag){
            System.out.println("Prime number");
        }
        else{
            System.out.println("Not a Prime number");
        }
    }
}




#### Sum of digits of a number
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        while(n!=0){
            int d=n%10;
            sum+=d;
            n/=10;
        }
        System.out.println("Sum of digits:"+sum);
    }
}




#### Reverse a number
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        while(n!=0){
            int d=n%10;
            sum=sum*10+d;
            n/=10;
        }
        System.out.println("Reversed number:"+sum);
    }
}




#### Palindrome number
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        int m=n;
        while(n!=0){
            int d=n%10;
            sum=sum*10+d;
            n/=10;
        }
        if(sum==m){
            System.out.println("Palindrome number");
        }
        else{
            System.out.println("Not a Palindrome number");
        }
    }
}




#### Armstrong number
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        int c=0;
        int m=n;
        int l=n;
        while(n!=0){
            int r=n%10;
            c++;
            n/=10;
        }
        while(m!=0){
            int d=m%10;
            sum=sum+(int)Math.pow(d,c);
            m/=10;
        }
        if(sum==l){
            System.out.println("Armstrong number");
        }
        else{
            System.out.println("Not an Armstrong number");
        }
    }
}



#### Fibonacci Series
import java.io.*;
import java.util.Scanner;
class Code {
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a=0,b=1,c;
        System.out.print(a+" "+b+" ");
        
        while(n>2){
            c=a+b;
            System.out.print(c+" ");
            a=b;
            b=c;
            n--;
        }
        
    }
}




#### Factorial of a number
import java.io.*;
import java.util.Scanner;

class GFG {
	public static void main (String[] args) {
		int fact=1;
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		while(n!=0){
		    fact=fact*n;
		    n--;
		}
		System.out.println(fact);
	}
}




#### Power of a number
import java.io.*;
import java.util.Scanner;

class GFG {
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int base=s.nextInt();
		int exp=s.nextInt();
		while(exp!=0){
		    base=base*base;
		    exp--;
		}
		System.out.println(base);
	}
}


#### Factor of a number
import java.io.*;
import java.util.Scanner;

class GFG {
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		for(int i=1;i<=n/2;i++){
		    if(n%i==0){
		        System.out.println(i);
		    }
		}
		System.out.println(n);
	}
}


#### Prime factor of a number
import java.io.*;
import java.util.Scanner;

class GFG {
    public static int prime(int num){
        
		for(int j=2;j<num;j++){
		    if(num%j==0){
		        return 0;
		    }
	    }
        return 1;
    }
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		for(int i=2;i<=n;i++){
		    if(prime(i)==1){
		        int a=n;
		        while(a%i==0){
		            System.out.println(i);
		            a/=i;
		        }
		    }
		}
	}
}



#### Check whether a number is Strong number or not
import java.io.*;
import java.util.Scanner;
class GFG {
    public static int fact(int num){
        int f=1;
        while(num!=0){
            f*=num;
            num--;
        }
        return f;
    }
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int sum=0;
		int m=n;
		while(n!=0){
		    int d=n%10;
		    sum+=fact(d);
		    n/=10;
		}
		if(sum==m){
		    System.out.println("It is a strong number");
		}
		else{
		    System.out.println("It is not a strong number");
		}
	}
}




#### Check whether a number is perfect number or not
import java.io.*;
import java.util.Scanner;
class GFG {
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int sum=0;
		for(int i=1;i<n;i++){
		    if(n%i==0){
		    sum+=i;
		    }
		}
		if(sum==n){
		    System.out.println("It is a perfect number");
		}
		else{
		    System.out.println("It is not a perfect number");
		}
	}
}



#### Check whether a number is perfect square or not
import java.io.*;
import java.util.Scanner;
class GFG {
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		if(Math.floor(Math.sqrt(n))==Math.ceil(Math.sqrt(n))){
		    System.out.println("It is a perfect square");
		}
		else{
		    System.out.println("It is not a perfect square");
		}
	}
}



#### Check whether a number is automorphic number or not
import java.io.*;
import java.util.Scanner;
class GFG {
    public static int automorphic(int n){
        int m=n*n;
		while(n!=0){
		    if(n%10!=m%10){
		        return 0;
		    }
		    m/=10;
		    n/=10;
		}
		return 1;
    }
	public static void main (String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		
		if(automorphic(n)==1){
		    System.out.println("Automorphic number");
		}
		else{
		    System.out.println("Not an Automorphic number");
		}
	}
}


#### Harshad number
import java.io.*;
import java.util.Scanner;
class GFG {
	public static void main (String[] args) {
	    Scanner s=new Scanner(System.in);
	    int n=s.nextInt();
	    int m=n;
	    int sum=0;
	    while(n!=0){
	        int d=n%10;
	        sum+=d;
	        n/=10;
	    }
	    if(m%sum==0){
	        System.out.println("Harshad number");
	    }
	    else{
	        System.out.println("Not a Harshad number");
	    }
	}
}
